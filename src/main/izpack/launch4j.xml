<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>${project.build.directory}/${package.jar}</jar>
  <outfile>${project.build.directory}/ProtocolAnalyzerInst.exe</outfile>
  <errTitle>Java not yet installed, please install Java first</errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <manifest>${staging.dir}/installation/ProtocolAnalyzerInst.exe.manifest</manifest>
  <icon>${staging.dir}/lib/analyzer.ico</icon>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
  </jre>
  <versionInfo>
    <fileVersion>${version}.0.0</fileVersion>
    <txtFileVersion>${version}</txtFileVersion>
    <fileDescription>Protocol Analyzer Tool</fileDescription>
    <copyright>(c) Stefan Strömberg</copyright>
    <productVersion>${version}.0.0</productVersion>
    <txtProductVersion>${version}</txtProductVersion>
    <productName>ProtocolAnalyzer</productName>
    <companyName>NetHome</companyName>
    <internalName>ProtocolAnalyzer</internalName>
    <originalFilename>ProtocolAnalyzerInst.exe</originalFilename>
  </versionInfo>
</launch4jConfig>