<shortcuts>
  <programGroup defaultName="NetHome" location="applications"/>
  <shortcut
     name="ProtocolAnalyzer"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH/analyzer.sh"
     commandLine=""
     workingDirectory="$INSTALL_PATH"
     description="This starts the ProtocolAnalyzer"
     iconFile="$INSTALL_PATH/lib/analyzer.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="normal">
   </shortcut>
  <shortcut
     name="Uninstall"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH/Uninstaller/uninstall.sh"
     commandLine=""
     workingDirectory="$INSTALL_PATH/Uninstaller"
     description="This uninstalls the ProtocolAnalyzer"
     iconFile="$INSTALL_PATH/lib/uninstall.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="noShow">
   </shortcut>
</shortcuts>
